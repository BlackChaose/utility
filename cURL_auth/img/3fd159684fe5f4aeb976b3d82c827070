<script>jQuery(function ($) {

function toggleBlockingButton() {
    if ($('.popup__form .has-error').length == 0) {
        if ($('#login_form_email_popup_login').val() != '' && $('#login_form_pass_popup_login').val() != '') {
            return $('.popup__form button').removeAttr("disabled");
        }
    }
    $('.popup__form button').attr("disabled", "disabled");
}

$( ".popup__form input" ).on('blur keyup', function() {
    setTimeout(toggleBlockingButton, 200);
